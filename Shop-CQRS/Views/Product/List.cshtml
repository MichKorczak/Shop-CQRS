@model ProductListViewModel

@foreach (var p in Model.Products)
{
    <div>
        <h3>@p.Name</h3>
        @p.Description
        <h4>@p.Category</h4>
        <h4>@p.NumberOfIrems</h4>
        <h4>@p.Price</h4>
    </div>
}

<div page-model="@Model.PagingInfo" page-action="List"></div>
